  },
  "homepage": "https://github.com/npm-gui-base/web-client#readme",
  "devDependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "opn": "^4.0.2",
    "webpack-dev-server": "^1.16.2"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {
    "npm-gui-base-core": "https://github.com/npm-gui-base/core",
