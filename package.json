{
  "name": "npm-gui",
  "version": "1.0.0",
  "description": "npm-gui",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" ",
    "dev:server": "concurrently \"npm run dev:server:webpack\" \"npm run dev:server:nodemon\" ",
    "dev:server:webpack": "webpack --config webpack.server.config.js --watch --mode=development",
    "dev:server:nodemon": "nodemon --watch dist bin/npm-gui 0.0.0.0:9002",
    "dev:client": "webpack-dev-server --config webpack.client.config.js --hot --mode=development",
    "build": "webpack -p",
    "test:unit": "karma start test/unit/karma.conf.js --single-run",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "node node_modules/stylelint/dist/cli.js 'components/*/**.vue' *.scss --syntax scss",
    "lint:js": "eslint \"client/**/**.vue\" \"client/**/**.js\" \"server/**/**.js\" ",
    "e2e": "node test/e2e/runner.js",
    "test": "mocha --require ts-node/register tests/**.ts",
    "test:unit:core": "node node_modules/mocha/bin/mocha ./service/**/*.test.js --compilers js:babel-register --require babel-polyfill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm-gui-base/client.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/npm-gui-base/client/issues"
  },
  "homepage": "https://github.com/npm-gui-base/client#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.17.2",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "cross-spawn": "^5.1.0",
    "css-loader": "^1.0.0",
    "debounce": "^1.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.15.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.11.0",
    "normalize.css": "^8.0.0",
    "nsp": "^2.6.3",
    "open-iconic": "^1.1.1",
    "opn": "^5.1.0",
    "proxyquire": "^1.8.0",
    "request": "^2.81.0",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "url-loader": "^1.1.2",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.0.8",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-node-externals": "^1.7.2",
    "ws": "^6.1.0"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {}
}
